<!--
	Tabelle funzionali di Diapason da interrogare per ottenere valori di decodifica di alcune informazioni
	
	PROVINCE
	-->
<Query name="Lista-tabella-province" orderBy="1" pagesize="0" service="TBitem" count="yes">
    <Fields>
        <Field name="Codice-provincia"/>
        <Field name="Descrizione-provincia"/>
        <Field name="Codice-area-zona"/>
        <Field name="Codice-regione"/>
        <Field name="Descrizione-regione"/>
        <Field name="Lead-time"/>
    </Fields>
</Query>