<!-- 
	INTERROGAZIONE DATI COMMERCIALI SOGGETTO
	Questa API interroga Diapason e restituisce i dati generali del soggetto
	
@param $codice_soggetto {stringa} codice che identifica univocamente il soggetto dentro a Diapason
-->
<GFDCliCommGet>
	<GFITEMS>
		<DATI-COMMERCIALI-CLIENTE>
			<CLIENTE>$codice_soggetto</CLIENTE>
		</DATI-COMMERCIALI-CLIENTE>
	</GFITEMS>
</GFDCliCommGet>