<!-- 
	INTERROGAZIONE VALORE ORDINE
	Questa API interroga Diapason e restituisce il valore commerciale dell'ordine
	
@param $numero_ordine {stringa} codice che identifica univocamente l'ordine dentro a Diapason
-->
<GFDOrdCliValGet>
		<GFITEMS>
			<VALORE-ORDINI-FIDI-ANTICIPI>
				<NRO-ORDINE>$numero_ordine</NRO-ORDINE>
			</VALORE-ORDINI-FIDI-ANTICIPI>
		</GFITEMS>
	</GFDOrdCliValGet>
