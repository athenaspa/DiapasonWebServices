<!--
	RILASCIO ORDINE
	Dopo l'inserimento dell'ordine se lo si vuole rendere "operativo" bisogna rilasciarlo

@param $cliente_intestatario {string} codice identificativo a Diapason del cliente 
@param $numero_ordine {string} codice che identifica univocamente l'ordine dentro a Diapason



	-->
<GFDOrdCliRels2>
					<GFITEMS>
									<RILASCIO-E-STAMPA-ORDINI-OFFERTE>
											<CLIENTE-TESTATA>$cliente_intestatario</CLIENTE-TESTATA>
											<NUMERO-ORDINE-TESTATA>$numero_ordine</NUMERO-ORDINE-TESTATA>
											<STATO-I-E-F-G-P-Q-R-C>P</STATO-I-E-F-G-P-Q-R-C>
											<STAMPA-ALLEGATO-S-N>N</STAMPA-ALLEGATO-S-N>
											<STAMPANTE>POVW</STAMPANTE>
											<VISUALIZZA>N</VISUALIZZA>
											<COPIE>1</COPIE>
											<MODIFICA>N</MODIFICA>
											<TIPO-A-B-D-I>A</TIPO-A-B-D-I>
									</RILASCIO-E-STAMPA-ORDINI-OFFERTE>
							</GFITEMS>
					</GFDOrdCliRels2>






